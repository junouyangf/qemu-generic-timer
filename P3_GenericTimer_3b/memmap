ENTRY(_start)
 
SECTIONS
{
    /* Starts at LOADER_ADDR. */
    . = 0x80000;

    .text :
    {
        *(.text)
    }
    . = ALIGN(4096); /* align to page size */

     .data :
    {
        *(.data)
    }
    . = ALIGN(4096); /* align to page size */
 
    .bss :
    {
        bss = .;
        *(.bss)
    }
    . = ALIGN(4096); /* align to page size */

}
